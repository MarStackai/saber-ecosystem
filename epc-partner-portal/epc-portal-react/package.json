{
  "name": "saber-epc-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_FORCE_WASM_BINARY=1 XDG_CACHE_HOME=.cache next dev -H 0.0.0.0 -p 4200",
    "build": "cross-env NEXT_FORCE_WASM_BINARY=1 XDG_CACHE_HOME=.cache next build",
    "deploy": "npx wrangler pages deploy ./out",
    "api:dev": "XDG_CONFIG_HOME=$PWD/.config WRANGLER_HOME=$PWD/.wrangler-home npx wrangler pages dev ./dev-static --ip 127.0.0.1 --port 8787",
    "start": "next start -p 4200",
    "lint": "next lint",
    "test": "jest",
    "test:admin": "jest tests/puppeteer/admin-processes.test.js",
    "test:partner": "jest tests/puppeteer/epc-partner-processes.test.js",
    "test:sharepoint": "jest tests/puppeteer/tender-sharepoint-integration.test.js --verbose",
    "test:sharepoint-api": "jest tests/api/sharepoint-api-integration.test.js --verbose",
    "test:admin-complete": "jest tests/puppeteer/admin-portal-complete.test.js --verbose",
    "test:partner-auth": "jest tests/puppeteer/partner-portal-auth.test.js --verbose",
    "test:complete-suite": "jest tests/puppeteer/admin-portal-complete.test.js tests/puppeteer/partner-portal-auth.test.js --verbose",
    "test:generate-docs": "node tests/fixtures/generate-test-documents.js",
    "test:all": "jest tests/puppeteer/*.test.js --verbose",
    "test:smoke": "./run-tests.sh"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@algolia/autocomplete-core": "^1.19.2",
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@markdoc/markdoc": "^0.5.2",
    "@markdoc/next.js": "^0.5.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@upstash/redis": "^1.35.3",
    "clsx": "^2.1.1",
    "fast-glob": "^3.3.3",
    "flexsearch": "^0.8.205",
    "js-yaml": "^4.1.0",
    "next": "^15",
    "next-themes": "^0.4.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^19",
    "react-dom": "^19",
    "react-highlight-words": "^0.21.0",
    "redis": "^5.8.2",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@types/jest": "^30.0.0",
    "@types/puppeteer": "^5.4.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15",
    "form-data": "^4.0.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "puppeteer": "^24.22.0",
    "sharp": "0.34.3"
  }
}
